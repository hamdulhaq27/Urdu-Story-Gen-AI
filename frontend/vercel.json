{
  "version": 2,
  "name": "frontend",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_API_URL": "http://YOUR_SERVER_IP:8000"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://YOUR_SERVER_IP:8000/:path*"
    }
  ]
}